			"method": "POST",
			"route": "/admin/login/h/",
			"controller": "AuthController@login",
			"name": "authLogin"
		},
		{
			"method": "GET",
			"route": "/admin/logout/h/",
			"controller": "AdminController@logout",
			"name": "adminLogout"
		},
