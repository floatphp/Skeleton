{
	"routes": [
		{
			"method": "GET",
			"route": "/api/v[i:version]/",
			"controller": "ApiController@index",
			"name": "ApiControllerIndex"
		},
		{
			"method": "GET",
			"route": "/",
			"controller": "HomeController@index",
			"name": "homeIndex"
		},
		{
			"method": "GET",
			"route": "/index.php",
			"controller": "HomeController@redirectIndex",
			"name": "redirectIndex"
		},
		{
			"method": "GET",
			"route": "/admin/login/h/",
			"controller": "AuthController@index",
			"name": "authIndex"
		},
